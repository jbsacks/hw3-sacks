<h1> Places I've Been! </h1>

<p> <a href="posts/new">Create a new post</a></p>

<p>
<div class="row">
  <% for post in @posts %>
    <div class="col-3">
      <h4><%= post["author"] %></h4>
      <img class="img-fluid" src="<%= post["image"] %>" class="w-full">
      <p><%= post["body"] %></p>
    </div>
  <% end %>
</div>
</p>